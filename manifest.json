{
	"$schema": "https://json.schemastore.org/web-manifest-combined.json",
	"name": "Tic Tac Toe", 
	"short_name": "TicTacToe", 
	"description": "A simple Tic Tac Toe game made with pure JavaScript and HTML.",
	"theme_color": "#fff",
	"background_color": "#000",
	"display": "standalone",
	"orientation": "portrait",
	"start_url": ".", 
	"scope": "/", 
	"icons": [
		{
			"src": "/static/favicon128.png", 
			"sizes": "128x128", 
			"type": "image/png", 
			"purpose": "any"
		}, 
		{
			"src": "/static/favicon192.png", 
			"sizes": "192x192", 
			"type": "image/png", 
			"purpose": "any maskable"
		}, 
		{
			"src": "/static/favicon256.png", 
			"sizes": "256x256", 
			"type": "image/png", 
			"purpose": "any"
		}, 
		{
			"src": "/static/favicon512.png", 
			"sizes": "512x512", 
			"type": "image/png", 
			"purpose": "any"
		}
	]
}